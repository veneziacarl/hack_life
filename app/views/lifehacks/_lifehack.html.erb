<div class="row">
<!-- Figure out to nest this "fixed elemnt projfsperly" -->
  <div class="large-8 columns  clearfix">
    <% @lifehacks.each do |lifehack| %>
    <div class="panel right large-12 columns">
      <h5><%= link_to lifehack.title, lifehack_path(lifehack) %></h5>
      <table>
        <tbody>
          <tr>
            <td><%= "#{lifehack.created_at.strftime("%m/%d/%Y")} #{lifehack.created_at.strftime("%I:%M%p")}"%></td>
            <td>Submitted By: <%= lifehack.creator.first_name %> </td>
            <td>Average Rating:</td>
            <td>Description: <%= lifehack.short_description %> ... </td>
          </tr>
        </tbody>

      </table>

    </div>
    <% end %>
  </div>
</div>
